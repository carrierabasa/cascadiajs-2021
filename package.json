{
  "name": "cascadiajs-2021",
  "version": "0.0.93",
  "description": "The web site & app for CascadiaJS 2021 - https://2021.cascadiajs.com",
  "scripts": {
    "start": "npx sandbox",
    "lint": "eslint src --ignore-pattern node_modules --fix",
